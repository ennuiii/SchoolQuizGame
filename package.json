{
  "name": "school-quiz-game",
  "version": "1.0.0",
  "description": "Multiplayer educational game for German school system",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev:client": "react-scripts start",
    "dev:server": "cd server && node index.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "render-build": "npm install --legacy-peer-deps && npm run build && mkdir -p server/build && cp -r build/* server/build/ && cd server && npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "ajv": "^6.12.4",
    "ajv-keywords": "^3.5.2",
    "bootstrap": "^5.2.3",
    "dotenv": "^16.5.0",
    "fabric": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "^3.0.1",
    "socket.io-client": "^4.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/fabric": "^5.3.10",
    "@types/node": "^22.15.8",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/uuid": "^9.0.1",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

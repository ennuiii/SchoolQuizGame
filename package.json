{
  "name": "school-quiz-game",
  "version": "1.0.0",
  "description": "Multiplayer educational game for German school system",
  "main": "server/dist/index.js",
  "scripts": {
    "start": "node server/dist/index.js",
    "dev:client": "react-scripts start",
    "dev:server": "cd server && npm run dev",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "react-scripts build",
    "build:server": "cd server && npm run build",
    "build:all": "npm run build:server && npm run build",
    "setup": "npm run build:server && npm start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "render-build": "npm install --legacy-peer-deps && cd server && npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@supabase/supabase-js": "^2.39.3",
    "@types/lodash": "^4.17.16",
    "@types/react-toastify": "^4.0.2",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.13.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "fabric": "^5.3.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.1",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "typescript": "^4.9.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/fabric": "^5.3.10",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/react-icons": "^3.0.0",
    "@types/uuid": "^9.0.8",
    "concurrently": "^7.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

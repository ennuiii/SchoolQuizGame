{
  "name": "school-quiz-game-server",
  "version": "1.0.0",
  "description": "Server for School Quiz Game",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src -e ts --exec \"npm run build && npm start\"",
    "build": "tsc",
    "clean": "rm -rf dist",
    "server:dev": "node build.js",
    "server:watch": "nodemon --watch src -e ts --exec \"node build.js\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}

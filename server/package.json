{
  "name": "school-quiz-game-server",
  "version": "1.0.0",
  "description": "Server for School Quiz Game",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec \"node ./node_modules/ts-node/dist/bin.js\" src/index.ts",
    "build": "tsc",
    "start:js": "node index.js",
    "dev:js": "nodemon index.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.22",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.8",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
